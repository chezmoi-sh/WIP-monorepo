{
    "compilerOptions": {
        "allowArbitraryExtensions": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ES2022",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "outDir": "out",
        "pretty": true,
        "sourceMap": true,
        "strict": true,
        "target": "ES2022"
    },
    "files": ["folderinfo.ts"]
}
